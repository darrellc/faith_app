try{
	var container = $("#itemBox .item-list");
	<% if empty.nil? %>
		createAlert({
			type: "success",
			msg: "<%= msg %>"		
		});	
		if($("#addEventForm #event_name").val() === "")
			$("#addEventForm #event_name").val("<%= e.name %>");
		var html = "";
		var counter = 0;
		html = "<%= e.generateEventItems true %>";
		$(container).append(html);		
	<% else %>
		$(container).empty();
	<% end %>
	//enable the select box
	$("#event_template_id").removeClass("disabled");
}catch(err){
	alert(err.message + err.stack);
}
		