<% if success %>
	toggleModal("#confirmDialog");	
	$("#eventTableData tr[data-id=<%= e.id %>]").velocity("fadeOut");
	
	createAlert({
		type: "success",
		msg: "The event was successfully deleted",
		timeout: 3000
	});
<% else %>
	toggleModal("#confirmDialog");
	createAlert({
		type: "alert",
		msg: "The event was not deleted.",
		timeout: 3000
	});
<% end %>
