toggleModal("#confirmModal", "close");
<% if success %>		
	$("#eventTableData tr[data-id=<%= e.id %>]").velocity("fadeOut");
	$("#eventTemplateTableData tr[data-id=<%= e.id %>]").velocity("fadeOut");	
	createAlert({
		type: "success",
		msg: "The event was successfully deleted",
		timeout: 1000
	});
<% else %>	
	createAlert({
		type: "alert",
		msg: "The event was not deleted.",
		timeout: 3000
	});
<% end %>
